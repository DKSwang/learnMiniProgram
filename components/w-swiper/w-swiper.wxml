<!--components/w-swiper/w-swiper.wxml-->
<swiper 
       circular
       autoplay
       interval="3000"
       duration="300"
       indicator-dots
       indicator-active-color="#ff5777"
       >
        <block wx:for="{{list}}" wx:key="index">
          <swiper-item class="swiper-item">
            <image  src="{{item.image}}" alt="" mode="widthFix"/>
            <view>{{item.title}}</view>
          </swiper-item>
        </block>
      </swiper>
